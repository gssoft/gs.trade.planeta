<Window x:Class="GS.Trade.Windows.QuotesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Quotes" Height="300" Width="200" Loaded="WindowLoaded"  Closed="WindowClosed">
    <Grid>
        <ListView Margin="0" Name="lstQuotes">
            <ListView.View>
                <GridView>
                    <GridView.Columns>
                        <GridViewColumn Header="Ticker" DisplayMemberBinding="{Binding Path=Ticker}" />
                        <GridViewColumn Header="DateTime" DisplayMemberBinding="{Binding Path=DateTimeString}" />
                        <GridViewColumn Header="Last" DisplayMemberBinding="{Binding Path=Last}" />
                    </GridView.Columns>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
