@model IEnumerable<GS.Trade.DataBase.Model.Ticker>

@{
    ViewBag.Title = "Tickers";
}

<h2>Tickers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Alias)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Decimals)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinMove)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TradeBoard)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BaseContract)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StrKey)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alias)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Decimals)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinMove)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TradeBoard)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BaseContract)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StrKey)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
