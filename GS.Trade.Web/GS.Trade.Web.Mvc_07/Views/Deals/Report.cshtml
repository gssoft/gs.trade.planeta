                @Html.DisplayFor(model => model.TotalTradesStr)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.NumberWinTrades)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NumberWinTradesStr)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.NumberLosingTrades)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NumberLosingTradesStr)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.PercentProfitable)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.PercentProfitableStr)
            </dd>
        </dl>
        <dl class="dl-horizontal">
