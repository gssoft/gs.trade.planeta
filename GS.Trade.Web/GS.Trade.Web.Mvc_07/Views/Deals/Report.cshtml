@model GS.Trade.Trades.Equity.OmegaReport
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Performance Report</h4>
<div>
    <strong>@ViewBag.FullTitle</strong>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Start DateTime
        </dt>
        <dd>
            @Html.DisplayFor(model => model.StartDate)
        </dd>
        <dt>
            End DateTime
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EndDate)
        </dd>
        <dt>
            Days
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Days)
        </dd>
        <dt>
            AvgDailyProfit
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AvgDailyProfit)
        </dd>
    </dl>
        <dl class="dl-horizontal">

            <dt>
                @Html.DisplayNameFor(model => model.TotalNetProfit)
            </dt>

